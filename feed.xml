        <link>http://ws1.corp.ailabs.tw:4000/2017/08/09/BaseballM.1502288659.A.C5C.html</link>
        <guid isPermaLink="true">http://ws1.corp.ailabs.tw:4000/2017/08/09/BaseballM.1502288659.A.C5C.html</guid>
        
        <category>Baseball</category>
        
        
