        <link>http://ws1.corp.ailabs.tw:4000/2017/10/17/BaseballM.1508218318.A.E1F.html</link>
        <guid isPermaLink="true">http://ws1.corp.ailabs.tw:4000/2017/10/17/BaseballM.1508218318.A.E1F.html</guid>
        
