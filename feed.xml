        <link>http://ws1.corp.ailabs.tw:4000/2017/09/03/GossipingM.1504415521.A.5AB.html</link>
        <guid isPermaLink="true">http://ws1.corp.ailabs.tw:4000/2017/09/03/GossipingM.1504415521.A.5AB.html</guid>
        
        <category>Gossiping</category>
        
        
