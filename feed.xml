        <link>http://ws1.corp.ailabs.tw:4000/2017/08/18/BaseballM.1503067406.A.B4A.html</link>
        <guid isPermaLink="true">http://ws1.corp.ailabs.tw:4000/2017/08/18/BaseballM.1503067406.A.B4A.html</guid>
        
