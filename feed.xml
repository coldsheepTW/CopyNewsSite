        <link>http://ws1.corp.ailabs.tw:4000/2017/08/18/BaseballM.1503067077.A.236.html</link>
        <guid isPermaLink="true">http://ws1.corp.ailabs.tw:4000/2017/08/18/BaseballM.1503067077.A.236.html</guid>
        
        <category>Baseball</category>
        
        
