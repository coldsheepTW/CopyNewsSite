        <link>http://ws1.corp.ailabs.tw:4000/2017/09/07/BaseballM.1504795044.A.501.html</link>
        <guid isPermaLink="true">http://ws1.corp.ailabs.tw:4000/2017/09/07/BaseballM.1504795044.A.501.html</guid>
        
