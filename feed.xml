        <link>http://ws1.corp.ailabs.tw:4000/2017/08/15/BaseballM.1502780657.A.948.html</link>
        <guid isPermaLink="true">http://ws1.corp.ailabs.tw:4000/2017/08/15/BaseballM.1502780657.A.948.html</guid>
        
