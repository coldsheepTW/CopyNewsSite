        <link>http://ws1.corp.ailabs.tw:4000/2017/08/11/BaseballM.1502382670.A.39C.html</link>
        <guid isPermaLink="true">http://ws1.corp.ailabs.tw:4000/2017/08/11/BaseballM.1502382670.A.39C.html</guid>
        
